<?xml version="1.0" encoding="utf-8"?>
<odoo noupdate="1">
    <record id="payment_method_tabby_installments" model="payment.method">
        <field name="name">Pay later with Tabby</field>
        <field name="code">tabby_installments</field>
        <field name="sequence">1000</field>
        <field name="active">False</field>
        <field name="image" type="base64" file="payment_tabby/static/description/icon.png"/>
        <field name="image_payment_form" type="base64" file="payment_tabby/static/description/icon_checkout.png"/>
        <field name="support_tokenization">False</field>
        <field name="support_express_checkout">False</field>
        <field name="support_manual_capture">partial</field>
        <field name="support_refund">partial</field>
        <field
            name="supported_country_ids"
            eval="[
                Command.set([
                    ref('base.ae'),
                    ref('base.sa'),
                    ref('base.kw'),
            ])]"
        />
        <field
            name="supported_currency_ids"
            eval="[
                Command.set([
                    ref('base.AED'),
                    ref('base.SAR'),
                    ref('base.KWD'),
            ])]"
        />
    </record></odoo>
